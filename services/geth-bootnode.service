[Unit]
Description=Ethereum go client
After=syslog.target network.target

[Service]
User=intern
Group=intern
WorkingDirectory=/home/intern
Type=simple
EnvironmentFile=/usr/lib/systemd/system/geth-bootnode.env

# ExecStart= /usr/local/bin/geth --datadir ${DATA_DIR} --networkid ${NET_ID} --nodekey ${NODE_KEY} --nat extip:${IP}
ExecStart=pwd

KillMode=process
KillSignal=SIGINT
TimeoutStopSec=90
Restart=on-failure
RestartSec=10s

[Install]
WantedBy=multi-user.target